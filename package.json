{
	"name": "agili",
	"publisher": "fbehrens",
	"repository": {
		"type": "git",
		"url": "https://github.com/fbehrens/agili.git"
	},
	"displayName": "agili",
	"description": "Tools for Agile-Development",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "agili.saveAndRepeat",
				"title": "agili: Save and Repeat last Command in Terminal"
			},
			{
				"command": "agili.saveAllAndRepeat",
				"title": "agili: Save All and Repeat last Command in Terminal"
			},
			{
				"command": "agili.play",
				"title": "agili: Play Set Clipboard to foo"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "watch1": "mocha --watch --watch-files src/**/*.ts -r ts-node/register src/myTest/**/*.test.ts",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/chai": "^4.2.11",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"chai": "^4.2.0",
		"ts-node": "^8.8.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
